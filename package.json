{
  "name": "eslint-plugin-detachhead",
  "version": "1.0.0",
  "description": "detachhead's eslint rules",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "detachhead",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:check": "eslint .",
    "lint:fix": "npm run lint:check -- --fix",
    "build": "rimraf ./dist && tsc -p src/tsconfig.json",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "ts-mocha -p tsconfig.json tests/**/*.ts --timeout 10000",
    "check": "npm run build && npm run lint:check && npm run lint:eslint-docs && npm run format:check && npm run test",
    "update:eslint-docs": "npm run build && eslint-doc-generator"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.53.0",
    "@typescript-eslint/utils": "^5.53.0",
    "eslint": "^8.19.0",
    "tsutils": "^3.21.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@detachhead/eslint-config": "^0.0.17",
    "@detachhead/ts-helpers": "^15.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/eslint": "^8.4.6",
    "@types/node": "^16.10.5",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-mocha": "^10.0.0"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "ISC"
}
